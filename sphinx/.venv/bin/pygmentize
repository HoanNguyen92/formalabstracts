#!/home/pv/Downloads/projects/formalabstracts/sphinx/.venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Pygments==2.1a0.dev20180612','console_scripts','pygmentize'
__requires__ = 'Pygments==2.1a0.dev20180612'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Pygments==2.1a0.dev20180612', 'console_scripts', 'pygmentize')()
    )
